PORT=8000
NODE_ENV=devolopment

# Mongodb
MONGO_URI=your_mongodb_key
# DATA ENCRYPTION KEY
DATA_ENCRYPTION_KEY=your_encryption_key

# Cloudinary 
CLOUDINARY_NAME=your_cloudinary_name_key
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_SECRET_KEY=your_cloudinary_secret_key


ADMIN_URL=http://localhost:4174
CLIENT_URL=http://localhost:4173
# JWT Secret Key
JWT_SECRET=your_jwt_secret
# Admin Account(Default)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=AdminPassword@123

# Paypal Paymnent
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_WEBHOOK_ID=your_webhook_id
PAYPAL_WEBHOOK_SECRET=your_paypal_webhook_secret

# VNPay Payment 
VNPAY_TMN_CODE=your_vnpay_tmn_code
VNPAY_HASH_SECRET=your_vnpay_hash_secret
VNPAY_PAYMENT_URL=https://sandbox.vnpayment.vn/paymentv2/vpcpay.html
VNPAY_RETURN_URL=http://localhost:8000/api/payment/vnpay/return
VNPAY_IPN_URL=http://localhost:8000/api/payment/vnpay_ipn
VNPAY_LOCALE=vn

# Gmail for send mail
GMAIL_CLIENT_ID=your_gmail_client_id
GMAIL_CLIENT_SECRET=your_client_secret
GMAIL_REFRESH_TOKEN=your_gmail_refresh_token
GMAIL_EMAIL=your_email

# GHN for shipping
GHN_TOKEN=your_GHN_Token
GHN_SHOP_ID=your_shop_id_in_GHN

# Rate limiting
# Comma-separated IP list to bypass rate limit (e.g., 127.0.0.1,10.0.0.2)
RATE_LIMIT_WHITELIST=
RATE_LIMIT_LOG_BLOCKED=true
# Global: 100 req / 15m per IP
RATE_LIMIT_GLOBAL_POINTS=100
RATE_LIMIT_GLOBAL_DURATION=900
# Auth/login: 5 req / 5m per IP
RATE_LIMIT_AUTH_POINTS=5
RATE_LIMIT_AUTH_DURATION=300
# Payment/checkout: 30 req / 10m per IP
RATE_LIMIT_PAYMENT_POINTS=30
RATE_LIMIT_PAYMENT_DURATION=600
